package org.dejava.component.util.test.runner.statement;

import org.dejava.component.util.test.annotation.MultiDataTest;
import org.dejava.component.util.test.runner.MultiDataJUnitRunner;

/**
 * Defines how test data must be provided for the {@link MultiDataJUnitRunner} and
 * {@link InvokeMultiDataTestMethod}.
 */
public interface TestDataProvider {
	
	public Iterable<?> getTestData(final MultiDataTest multiDataTest);
}
