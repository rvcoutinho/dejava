<?xml version="1.0" encoding="ISO-8859-1"?>
<testCase>
    <name>Get superclasses - Valid parameters</name>
    <description>Tests the "get superclasses" method with valid parameters.</description>
    <steps>
        <step class="org.dejava.component.util.test.model.TestStep">
            <name>Get superclasses - static call with expected return</name>
            <description>Calls the "get superclasses" method from the static class and expects a given return.</description>
            <method>
                <staticMethodClass>org.dejava.component.util.reflection.handler.ClassHandler</staticMethodClass>
                <name>getSuperclasses</name>
                <parametersClasses>
                    <parameterClass class="java.lang.Class">java.lang.Class</parameterClass>
                </parametersClasses>
                <parametersValues>
                    <parameterValue>#{object.clazz}</parameterValue>
                </parametersValues>
                <expectedReturn>#{object.superclasses}</expectedReturn>
            </method>
        </step>
    </steps>
    <data>
        <data
			class="org.dejava.component.util.reflection.test.clazz.auxiliary.ClassSuperclasses">
