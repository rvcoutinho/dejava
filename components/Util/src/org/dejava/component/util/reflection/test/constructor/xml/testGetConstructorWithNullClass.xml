<?xml version="1.0" encoding="ISO-8859-1"?>
<testCase>
    <name>Get constructor - Null class</name>
    <description>Tests the method "get constructor" with null class.</description>
    <steps>
        <step class="org.dejava.component.util.test.model.TestStep">
            <name>Get constructor - Static call with null class</name>
            <description>Calls the "get constructor" method from the static class with null class and expects a given exception</description>
            <method>
                <staticMethodClass>org.dejava.component.util.reflection.handler.ConstructorHandler</staticMethodClass>
                <name>getClass</name>
                <parametersClasses>
                    <parameterClass class="java.lang.Class">java.lang.Class</parameterClass>
                    <parameterClass class="java.lang.Class">java.lang.Class[]</parameterClass>
                </parametersClasses>
                <parametersValues>
                    <parameterValue>#{null}</parameterValue>
                </parametersValues>
                <expectedException>
                    <name>EmptyParameterException</name>
                    <message>reflection.class.name.empty</message>
                </expectedException>
            </method>
        </step>
    </steps>
</testCase>